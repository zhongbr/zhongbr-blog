{
  "name": "react-press",
  "version": "1.0.0",
  "scripts": {
    "install-global": "pnpm install",
    "install-website": "cd ./packages/website && pnpm install",
    "build-website": "cd ./packages/website && pnpm run build",
    "start-website": "cd ./packages/website && pnpm run start",
    "build-markdown": "node ./scripts/markdown-transform/index.js",
    "install-pack-umd": "cd ./packages/umd-factory && pnpm install",
    "build-umd": "cd ./packages/umd-factory && pnpm run compile",
    "pack-umd": "cd ./packages/umd-factory && pnpm run pack $PACKAGES_PATH",
    "install-jsx-core": "cd ./packages/jsx-core && pnpm install",
    "build-jsx-worker": "cd ./packages/jsx-core && pnpm run worker",
    "build-jsx-core": "cd ./packages/jsx-core && pnpm run compile",
    "start-jsx-core": "cd ./packages/jsx-core && pnpm run start"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "md2json": "workspace:^1.0.0"
  },
  "dependencies": {
    "chalk": "github:chalk/chalk",
    "glob": "^8.0.3",
    "rmraf": "^1.0.3"
  }
}
